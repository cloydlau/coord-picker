{
  "name": "coord-picker",
  "PascalCasedName": "CoordPicker",
  "version": "0.8.3",
  "private": false,
  "packageManager": "pnpm@7.23.0",
  "description": "Coordinates picker for AMap & TMap.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/coord-picker",
  "keywords": [
    "map",
    "AMap",
    "TMap",
    "coordinates"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/coord-picker.umd.js",
      "import": "./dist/coord-picker.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/coord-picker.umd.js",
  "module": "./dist/coord-picker.mjs",
  "unpkg": "./dist/coord-picker.umd.js",
  "jsdelivr": "./dist/coord-picker.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npx lint-staged && pnpm build && cl release"
  },
  "peerDependencies": {
    "element-ui": ">=2",
    "vue": "2"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@tarekraafat/autocomplete.js": "^9.1.1",
    "kikimore": "^0.11.1",
    "lodash-es": "^4.17.21",
    "pic-viewer": "^0.8.4",
    "sweetalert2": "^11.6.16",
    "sweetalert2-preset": "^0.2.2",
    "vue-global-config": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@iconify/json": "^2.2.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue2": "^2.2.0",
    "case-police": "^0.5.10",
    "element-ui": "^2.15.12",
    "husky": "^8.0.3",
    "json-editor-vue": "^0.10.5",
    "lint-staged": "^13.1.0",
    "rome": "^11.0.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.14.15",
    "vanilla-jsoneditor": "^0.11.8",
    "vite": "^4.0.4",
    "vite-plugin-css-injected-by-js": "^2.2.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^2.7.14",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": "rome check",
    "*.md": [
      "case-police --fix",
      "zhlint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
