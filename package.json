{
  "name": "coord-picker",
  "version": "0.7.1",
  "author": "cloydlau",
  "license": "MIT",
  "description": "Coordinates picker for AMap & TMap / 高德 & 腾讯地图坐标拾取工具",
  "keywords": [
    "地图选点",
    "坐标拾取",
    "高德地图",
    "腾讯地图",
    "多边形",
    "轮廓",
    "coordinate",
    "coordinates",
    "amap",
    "tmap"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cloydlau/coord-picker"
  },
  "main": "dist/coord-picker.umd.js",
  "module": "./dist/coord-picker.mjs",
  "exports": {
    ".": {
      "import": "./dist/coord-picker.mjs",
      "require": "./dist/coord-picker.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward",
    "test": "vitest --config ./vitest.config.ts",
    "test2": "vitest --config ./vitest.vue2.config.ts",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "latest",
    "@tarekraafat/autocomplete.js": "^9.1.1",
    "cozyalert": "latest",
    "kayran": "latest",
    "kikimore": "latest",
    "lodash-es": "latest",
    "pic-viewer": "latest",
    "vue-global-config": "latest"
  },
  "peerDependencies": {
    "element-ui": "2",
    "vue": "2"
  },
  "devDependencies": {
    "@iconify/json": "latest",
    "@vue/composition-api": "latest",
    "chalk": "^4.1.2",
    "element-ui": "latest",
    "enquirer": "latest",
    "execa": "^4.1.0",
    "json-editor-vue": "latest",
    "minimist": "latest",
    "sass": "latest",
    "semver": "latest",
    "typescript": "latest",
    "unocss": "latest",
    "unplugin-icons": "latest",
    "vite": "latest",
    "vite-plugin-vue2": "latest",
    "vue": "2",
    "vue-template-compiler": "latest"
  }
}
